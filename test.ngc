let #a = 37
let #b = 73

sub mul[#a, #b, #c, #d]
    sub add[#a, #b]
        return #a + #b
    endsub

    return add[#a, #b] * add[#c, #d]
endsub

let #c = mul[100, 101, 102, 103] + #a
print["#c: ", #c]

